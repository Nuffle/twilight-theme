	"tokenColors": [
		// {
		// 	"name": "Function Parameters",
		// 	"scope": [
		// 	  "variable.parameter"
		// 	],
		// 	"settings": {
		// 	  "foreground": "#bafdff"
		// 	}
		//  },
		{
			"name": "Boolean Literals",
			"scope": [
				"constant.language.boolean",
				"constant.language.null",
				"constant.language"
			],
			"settings": {
				"foreground": "#00aeff"
			}
		},
		{
			"name": "Comments",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#9898D9"
			}
		},
		{
			"name": "Keywords",
			"scope": [
				"keyword",
				"storage.type",
				"storage.modifier"
			],
			"settings": {
				"foreground": "#00FFFF",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Strings",
			"scope": [
				"string",
				"constant.other.symbol",
				"punctuation.definition.string.begin",
				"punctuation.definition.string.end"
			],
			"settings": {
				"foreground": "#e5ff00"
			}
		},
		{
			"name": "Numbers",
			"scope": [
				"constant.numeric"
			],
			"settings": {
				"foreground": "#FF00FF"
			}
		},
		{
			"name": "Punctuation",
			"scope": [
				"punctuation",
				"punctuation.definition.bracket",
				"punctuation.definition.parameters",
				"punctuation.separator"
			],
			"settings": {
				"foreground": "#00FFFF"
			}
		}
	]
